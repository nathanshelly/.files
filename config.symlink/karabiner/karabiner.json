        "rules": [
          {
            "description": "Ctrl to Escape when tapped (Caps Lock mapped to Ctrl in simple modifications)",
            "manipulators": [
              {
                "from": {
                  "key_code": "right_control",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "right_control",
                    "lazy": true
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Change Right Cmd to Cmd-Ctrl-Alt (Hyper key)",
